{
  "name": "mongodb-log",
  "version": "1.2.5",
  "description": "Turn flat mongodb log entires into nice structured objects.",
  "scripts": {
    "build": "pegjs mongodb-log.pegjs lib/grammar.js",
    "check": "mongodb-js-precommit",
    "test": "mocha",
    "ci": "./node_modules/istanbul/lib/cli.js cover _mocha -- -R spec ./test/*.test.js"
  },
  "check": {
    "ignore": [
      "coverage/*",
      "bench/*",
      "bin/*",
      "lib/fix-unescaped-quotes.js"
    ]
  },
  "precommit": [
    "check",
    "test"
  ],
  "dependencies": {
    "ampersand-state": "~5.0.2",
    "debug": "~2.3.2",
    "event-stream": "~3.3.1",
    "json-literal": "~1.1.0",
    "lodash.clone": "^4.5.0",
    "lodash.isarray": "^4.0.0",
    "lodash.isobject": "^3.0.2",
    "lodash.isregexp": "^4.0.1",
    "lodash.map": "^4.6.0",
    "lodash.trim": "^4.5.1",
    "mongodb-log2ejson": "~0.1.3",
    "pegjs": "^0.10.0",
    "regret": "~0.1.2"
  },
  "devDependencies": {
    "benchmark": "~2.1.2",
    "cli-table": "~0.3.1",
    "coveralls": "^2.11.2",
    "eslint-config-mongodb-js": "^2.2.0",
    "istanbul": "^0.4.5",
    "mocha": "~3.1.2",
    "mongodb-js-precommit": "^0.2.9",
    "pre-commit": "^1.0.10"
  },
  "keywords": [
    "mongodb",
    "mongodb.js"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/mongodb-js/log.git"
  },
  "author": "Lucas Hrabovsky <hrabovsky.lucas@gmail.com> (http://imlucas.com)",
  "license": "Apache-2.0",
  "homepage": "http://github.com/mongodb-js/log"
}
